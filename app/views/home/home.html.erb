<h1>Welcome to fumblr</h1>
<p><%= link_to "Sign Up", new_user_path %></p> 
<p><%= link_to "Sign In", log_in_path %></p>

<h1>Log In:</h1>

<%= form_tag log_in_path do %>
<%= label_tag(:username) %>
<%= text_field_tag(:username) %>
<%= label_tag(:password) %>
<%= text_field_tag(:password) %>
<%= submit_tag("Log In") %>
<% end %>


